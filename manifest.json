{
  "name": "TreBis",
  "short_name": "TreBis",
  "description": "Расширение для автоматического создания списка в Trello. А также переносятся карточки предыдущего дня, расставляются нужные метки.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "js": [
        "dist/index.js"
      ]
    }
  ],
  "icons": {
    "36": "images/icon.png",
    "48": "images/icon-48.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "web_accessible_resources": [
    "images/*"
  ],
  "permissions": [
    "cookies",
    "webRequestBlocking",
    "https://trello.com/",
    "https://api.trello.com/"
  ],
  "manifest_version": 2
}
